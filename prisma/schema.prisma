generator client {
  provider = "prisma-client-js"
  output   = "../src/server/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  seq         BigInt @id @unique @default(autoincrement())
  publicKey   Bytes  @unique
  latestNonce Bytes?
}
